public class Calculator {
	

	public static void main(String[] args) {
		int a=5;
		for(int i=a;i>=0;i--)
		{
			for(int j=0;j<i;j++)
			
				System.out.print(i+" ");		
		
			System.out.println();
		
		}
		for(int i=0;i<=a;i++)
		{
			for(int j=0;j<i;j++)
				
				System.out.print(i+" ");		
		
			System.out.println();
		}
	}   
}  
